{
  "name": "@framersai/agentos-demo-automation",
  "version": "0.1.0",
  "private": true,
  "description": "Automated demo recording system for AgentOS Workbench",
  "scripts": {
    "record": "npx ts-node run.ts",
    "record:all": "npx ts-node run.ts --all",
    "record:agent": "npx ts-node run.ts --demo agent-creation",
    "record:multi": "npx ts-node run.ts --demo multi-agent",
    "record:rag": "npx ts-node run.ts --demo rag-memory",
    "record:planning": "npx ts-node run.ts --demo planning-engine",
    "record:streaming": "npx ts-node run.ts --demo streaming",
    "record:4k": "npx ts-node run.ts --width 3840 --height 2160",
    "record:1440p": "npx ts-node run.ts --width 2560 --height 1440",
    "process": "npx ts-node lib/video-processor.ts",
    "setup": "playwright install chromium"
  },
  "dependencies": {
    "ffmpeg-static": "^5.3.0",
    "openai": "^4.0.0",
    "playwright": "^1.48.0"
  },
  "devDependencies": {
    "@types/ffmpeg-static": "^5.1.0",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
